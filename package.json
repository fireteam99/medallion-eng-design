{
  "name": "medallion-eng-design",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build && npm run db-migrate && npm run db-seed",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "db-migrate": "sequelize-cli db:migrate",
    "db-migrate:undo": "sequelize-cli db:migrate:undo",
    "db-seed": "sequelize-cli db:seed:all",
    "db-seed:undo": "sequelize-cli db:seed:undo"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.2.8",
    "luxon": "^2.3.1",
    "next": "12.1.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "swr": "^1.2.2",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "2.6.0"
  }
}
